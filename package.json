{
  "name": "focus-window",
  "version": "1.0.0",
  "description": "This extension allows one to create various shortcuts for applications, enabling the ability to have one shortcut that triggers both the launch and focus of an application window.",
  "scripts": {
    "setup": "npm run setup:types",
    "setup:types": "npx @ts-for-gir/cli generate -e gjs -o ./src/@types",
    "test:prefs": "npm run test:build && npm run prefs && npm run test:log:prefs",
    "test:extension": "npm run enable && npm run test:build && dbus-run-session -- gnome-shell --nested --wayland",
    "test:build": "./build.sh -irv",
    "test:open": "",
    "test:log": "journalctl -f -o cat | awk '/focus-window@chris.al:/ {gsub(/^.*focus-window@chris.al:\\s/, \"\"); print }'",
    "test:log:prefs": "journalctl -f -o cat /usr/bin/gjs | awk '/focus-window@chris.al:/ {gsub(/^.*focus-window@chris.al:\\s/, \"\"); print }'",
    "test:log:extension": "journalctl -f -o cat GNOME_SHELL_EXTENSION_UUID=focus-window@chris.al | awk '/focus-window@chris.al:/ {gsub(/^.*focus-window@chris.al:\\s/, \"\"); print }'",
    "uninstall": "gnome-extensions uninstall focus-window@chris.al",
    "prefs": "gnome-extensions prefs focus-window@chris.al",
    "enable": "gnome-extensions enable focus-window@chris.al",
    "disable": "gnome-extensions disable focus-window@chris.al",
    "build": "./build.sh -ptv",
    "build:install": "./build.sh -ptir",
    "settings": "dconf dump /org/gnome/shell/extensions/focus-window/",
    "settings:clear": "dconf reset -f /org/gnome/shell/extensions/focus-window/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pcbowers/focus-window.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pcbowers/focus-window/issues"
  },
  "homepage": "https://github.com/pcbowers/focus-window#readme",
  "devDependencies": {
    "@girs/gnome-shell": "^3.0.0",
    "@ts-for-gir/cli": "^3.0.4"
  }
}
